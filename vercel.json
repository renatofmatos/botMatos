{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/webhook",
        "dest": "/api/webhook",
        "methods": ["POST"]
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }