{
    "version": 2,
    "builds": [
      {
        "src": "dist/api/webhook.js",
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npm run build",
          "startCommand": "npm start"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/webhook",
        "dest": "/api/webhook",
        "methods": ["POST"]
      }
    ]
  }